{
  "name": "comma-cavalry-client",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "routify -b && rollup -c && routify export && mv dist/__app.html dist/index.html && cap copy",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "gen:resources": "RES='./scripts/resources' && npx cordova-res ios --resources $RES --icon-source $RES/icon-ios.png && npx cordova-res android --resources $RES --icon-source $RES/icon-android.png && node scripts/resources/generate.js",
    "rollup": "rollup -cw",
    "cg": "graphql-codegen",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@apollo/client": "^3.1.3",
    "@capacitor/cli": "^2.4.0",
    "@capacitor/core": "^2.4.0",
    "@graphql-codegen/add": "^2.0.0",
    "@graphql-codegen/cli": "^1.17.7",
    "@graphql-codegen/typescript": "^1.17.7",
    "@ollopa/cedar": "^0.1.9",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@tsconfig/svelte": "^1.0.8",
    "@types/hammerjs": "^2.0.36",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "graphql": "^15.3.0",
    "hammerjs": "^2.0.8",
    "jsfeat": "^0.0.8",
    "nollup": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.18.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-svelte-hot": "^0.9.2",
    "rollup-plugin-terser": "^6.1.0",
    "spassr": "^1.1.2",
    "svelte-check": "^1.0.10",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@capacitor/android": "2.4.0",
    "@capacitor/ios": "2.4.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@sveltech/routify": "^1.9.7",
    "postcss-import": "^12.0.1",
    "svelte": "^3.23.2",
    "svelte-preprocess": "^4.0.10",
    "workbox-cli": "^5.1.3"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "routifyDir": ".routify"
  }
}
